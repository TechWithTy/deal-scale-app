{
  "name": "deal_scale",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "CyberOni Software Solutions",
    "url": "https://github.com/techwithty"
  },
  "scripts": {
    "prebuild": "pnpm run check:build-prereqs",
    "check:build-prereqs": "node tools/checks/check-build-prereqs.cjs",
    "archive:coverage": "pnpm exec tsx tools/reports/archive-vitest-coverage.ts",
    "archive:owasp": "pnpm exec tsx tools/security/archive-owasp-report.ts",
    "archive:reports": "pnpm exec tsx tools/reports/archive-lighthouse-report.ts",
    "archive:security": "pnpm run archive:trivy && pnpm run archive:owasp",
    "archive:trivy": "pnpm exec tsx tools/security/archive-trivy-report.ts",
    "bdd": "set TS_NODE_PROJECT=tsconfig.cucumber.json&& cucumber-js --require-module ts-node/register/transpile-only --require-module tsconfig-paths/register --require lib/stores/user/_tests/_steps/support.ts",
    "bdd:stores": "set TS_NODE_PROJECT=tsconfig.cucumber.json&& cucumber-js --tags '@stores' --require-module ts-node/register/transpile-only --require-module tsconfig-paths/register --require lib/stores/user/_tests/_steps/support.ts",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:analyze:head": "ANALYZE=true pnpm next build 2>&1 | head -20",
    "build:cf": "pnpm exec next-on-pages",
    "build:rs": "pnpm run build:rspack",
    "build:rspack": "pnpm exec tsx scripts/bundlers/ensure-preview-flag.ts rspack:build && pnpm exec rsbuild build --config tools/rspack-preview/rsbuild.config.ts",
    "build:vite": "pnpm exec tsx scripts/bundlers/ensure-preview-flag.ts vite:build && pnpm exec vite build --config tools/vite-preview/vite.config.ts",
    "check:env": "pnpm exec tsx tools/checks/check-analytics-env.ts",
    "clean-build": "rm -rf .next dist .svelte-kit node_modules/.cache && pnpm build",
    "clean:cache": "node scripts/clear-next-cache.js",
    "clean:external": "node scripts/clean-external-node-modules.js",
    "commit:stage": "git add . && pnpm exec lint-staged",
    "dev": "pnpm run check:env && next dev",
    "dev-turbo": "pnpm run check:env && next dev --turbopack",
    "dev:rs": "pnpm run dev:rspack",
    "dev:rspack": "pnpm exec tsx scripts/bundlers/ensure-preview-flag.ts rspack:dev && pnpm exec rsbuild dev --config tools/rspack-preview/rsbuild.config.ts",
    "dev:vite": "pnpm exec tsx scripts/bundlers/ensure-preview-flag.ts vite:dev && pnpm exec vite --config tools/vite-preview/vite.config.ts",
    "doctor:rspack": "pnpm run build:rspack && pnpm dlx @rsdoctor/cli analyze --profile .rsdoctor/manifest.json",
    "format": "biome format --write src",
    "lint": "pnpm exec biome check --write src",
    "perf:lighthouse:local": "pnpm dlx lighthouse http://localhost:3000/ --preset=perf --throttling-method=simulate --max-wait-for-load=15000 --output=json --output=html --output-path=reports/lighthouse/local",
    "perf:lighthouse:local-auth": "pnpm tsx scripts/perf/create-lighthouse-headers.ts --base http://localhost:3000 --output reports/lighthouse/local.headers.json --dashboard /dashboard && pnpm dlx lighthouse http://localhost:3000/dashboard --preset=perf --throttling-method=simulate --max-wait-for-load=15000 --extra-headers=reports/lighthouse/local.headers.json --output=json --output=html --output-path=reports/lighthouse/local",
    "perf:lighthouse:local-ci": "pnpm tsx scripts/perf/run-local-perf.ts",
    "perf:lighthouse:prod": "pnpm dlx lighthouse https://app.dealscale.io/ --preset=perf --throttling.cpuSlowdownMultiplier=4 --throttling.downloadThroughputKbps=1500 --throttling.uploadThroughputKbps=750 --max-wait-for-load=15000 --output=json --output=html --output-path=reports/lighthouse/prod",
    "perf:lighthouse:prod-auth": "pnpm tsx scripts/perf/create-lighthouse-headers.ts --base https://app.dealscale.io --output reports/lighthouse/prod.headers.json --dashboard /dashboard && pnpm dlx lighthouse https://app.dealscale.io/dashboard --preset=perf --throttling.cpuSlowdownMultiplier=4 --throttling.downloadThroughputKbps=1500 --throttling.uploadThroughputKbps=750 --max-wait-for-load=15000 --extra-headers=reports/lighthouse/prod.headers.json --output=json --output=html --output-path=reports/lighthouse/prod",
    "postbuild": "pnpm run submit:sitemap || echo \"[postbuild] sitemap submission skipped (script missing)\"",
    "postinstall": "pnpm run sync:commit-scopes && pnpm run postinstall:submodules",
    "postinstall:submodules": "node scripts/postinstall.js",
    "prepare": "node ./scripts/setup/prepare-husky.cjs",
    "security:owasp": "pnpm exec tsx tools/security/run-owasp-baseline.ts",
    "security:scan": "pnpm run security:trivy && pnpm run security:owasp",
    "security:trivy": "pnpm exec tsx tools/security/run-trivy-scan.ts",
    "start": "pnpm run check:env && next start",
    "sync:commit-scopes": "pnpm exec tsx tools/commitlint/sync-scopes.ts",
    "test": "pnpm run test:vitest",
    "test:coverage": "pnpm exec vitest run --run --passWithNoTests",
    "test:critical": "pnpm exec tsx tools/tests/run-critical-tests.ts",
    "test:pwa": "vitest run tests/pwa/pwa.spec.ts --config vitest.config.ts --reporter=dot",
    "test:rspack": "pnpm exec tsx scripts/bundlers/ensure-preview-flag.ts rspack:test && pnpm run test:vitest",
    "test:stores": "vitest run \"lib/stores/__tests__/**/*.test.ts\" --config vitest.config.ts --reporter=dot",
    "test:stores:playwright": "playwright test lib/stores/user/_tests",
    "test:vitest": "pnpm exec vitest run --run --passWithNoTests",
    "test:watch": "vitest --watch --config vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "{app,components,lib,types,hooks,api,actions,external,packages}/**/*.{js,jsx,ts,tsx,json,css,md,html,scss}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
