#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules:/mnt/c/Users/tyriq/Documents/Github/deal-scale-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tailwindcss@3.4.17_ts-node@10.9.2_@swc+core@1.13.5_@types+node@20.5.7_typescript@5.9.2_/node_modules/tailwindcss/lib/cli.js" "$@"
fi
